{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [
				{
					"colors": ["#23872d", "#d2d700"],
					"name": "Color 1 + 2",
					"slug": "duotone-1-2"
				},
				{
					"colors": ["#23872d", "#F6F6F6"],
					"name": "Color 1 + 3",
					"slug": "duotone-1-3"
				},
				{
					"colors": ["#d2d700", "#F6F6F6"],
					"name": "Color 2 + 3",
					"slug": "duotone-2-3"
				}
			],
			"palette": [
				{
					"color": "#fafafa",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#23872d",
					"name": "Color 1",
					"slug": "color-1"
				},
				{
					"color": "#d2d700",
					"name": "Color 2",
					"slug": "color-2"
				},
				{
					"color": "#F6F6F6",
					"name": "Color 3",
					"slug": "color-3"
				}
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1920px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 0.5)",
					"name": "1",
					"slug": "10"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 1)",
					"name": "2",
					"slug": "20"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 2)",
					"name": "3",
					"slug": "30"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 3)",
					"name": "4",
					"slug": "40"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 4)",
					"name": "5",
					"slug": "50"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 5)",
					"name": "6",
					"slug": "60"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 6)",
					"name": "7",
					"slug": "70"
				}
			],
			"units": ["%","px","em","rem","vh","vw"]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.8rem",
						"max": "1.25rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.563rem"
					},
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.953rem"
					},
					"size": "1.563rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.563rem",
						"max": "2.441rem"
					},
					"size": "1.953rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "1.953rem",
						"max": "3.052rem"
					},
					"size": "2.441rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "2.441rem",
						"max": "3.815rem"
					},
					"size": "3.052rem",
					"slug": "xxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"link": {
				"color": {
					"text": "inherit"
				},
				":hover": {
					"typography": {
						"textDecoration": "dashed underline"
					}
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "400",
					"lineHeight": "1.333"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"right": "var(--wp--preset--spacing--20)",
				"left": "var(--wp--preset--spacing--20)"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.5"
		},
		"blocks": {
			"core/media-text": {
				"css": "{column-gap: var(--wp--preset--spacing--30);row-gap: var(--wp--preset--spacing--20);} & .wp-block-media-text__content {padding: 0;}"
			},
			"core/site-title": {
				"css": "& a {text-decoration: none;}"
			},
			"core/list": {
				"spacing": {
					"margin": {
						"left": "0",
						"right": "0"
					},
					"padding": {
						"left": "1em",
						"right": "1em"
					}
				}
			}
		},
		"templateParts": [
			{
				"area": "header",
				"name": "header",
				"title": "Header"
			},
			{
				"area": "footer",
				"name": "footer",
				"title": "Footer"
			}
		]
	}
}