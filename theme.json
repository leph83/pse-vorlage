{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1920px"
		},
		"color": {
			"defaultPalette": true,
			"palette": [
				{
					"color": "#fafafa",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#0a0a0a",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#23872d",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#d2d700",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#F6F6F6",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"spacing": {
			"spacingSizes": [
				{
					"size": "calc(var(--wp--preset--font-size--medium) / 4)",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) / 2)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "var(--wp--preset--font-size--medium)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 2)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 3)",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 4)",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "calc(var(--wp--preset--font-size--medium) * 5)",
					"slug": "70",
					"name": "7"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.64rem",
						"max": "1rem"
					},
					"size": "0.8rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "0.8rem",
						"max": "1.25rem"
					},
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.563rem"
					},
					"size": "1.25rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.25rem",
						"max": "1.953rem"
					},
					"size": "1.563rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "1.563rem",
						"max": "2.441rem"
					},
					"size": "1.953rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "1.953rem",
						"max": "3.052rem"
					},
					"size": "2.441rem",
					"slug": "xxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "400",
					"lineHeight": "1.333"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--40)",
			"padding": {
				"right": "var(--wp--preset--spacing--60)",
				"left": "var(--wp--preset--spacing--60)"
			}
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.5"
		},
		"blocks": {
			"core/media-text": {
				"css": "{\n\tgap: var(--wp--preset--spacing--40);\n}\n\n& .wp-block-media-text__content {\n\tpadding: 0;\n}"
			}
		}
	}
}